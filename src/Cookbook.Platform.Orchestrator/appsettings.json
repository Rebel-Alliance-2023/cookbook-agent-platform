{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MessagingBus": {
    "Implementation": "RedisSignalR",
    "RedisStreamPrefix": "stream:",
    "RedisKeyPrefix": "runs:",
    "RedisDb": 0
  },
  "Cosmos": {
    "DatabaseName": "cookbook-db",
    "UseEmulator": true
  },
  "Storage": {
    "UseAzurite": true,
    "ContainerName": "artifacts"
  },
  "Llm": {
    "DefaultProvider": "Claude",
    "OpenAI": {
      "Model": "gpt-4o"
    },
    "Anthropic": {
      "Model": "claude-3-5-sonnet-20241022"
    }
  }
}
