{
  "name": "Classic Chocolate Chip Cookies",
  "description": "Soft and chewy chocolate chip cookies with crispy edges. A timeless family favorite that's perfect for any occasion.",
  "prepTime": "PT15M",
  "cookTime": "PT12M",
  "totalTime": "PT27M",
  "recipeYield": "24 cookies",
  "recipeCategory": "Dessert",
