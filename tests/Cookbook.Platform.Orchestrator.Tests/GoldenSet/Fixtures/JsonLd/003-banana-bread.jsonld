{
  "name": "Classic Banana Bread",
  "description": "Moist and flavorful banana bread made with ripe bananas. Perfect for breakfast or as a snack with your coffee.",
  "prepTime": "PT15M",
  "cookTime": "PT60M",
  "totalTime": "PT1H15M",
  "recipeYield": "1 loaf (12 slices)",
  "recipeCategory": "Breakfast",
